<?xml version="1.0" encoding="utf-8"?>
<PARAMDEF XmlVersion="3">
  <ParamType>EVENT_MESSAGE_ST</ParamType>
  <DataVersion>1</DataVersion>
  <BigEndian>True</BigEndian>
  <Unicode>False</Unicode>
  <FormatVersion>101</FormatVersion>
  <Fields>
    <Field Def="u32 Message ID = 0">
      <DisplayName>メッセージID</DisplayName>
      <Description>この付加データを一意に決定するID</Description>
      <EditFlags>None</EditFlags>
      <Maximum>9900000</Maximum>
    </Field>
    <Field Def="u32 Text ID = 0">
      <DisplayName>テキストID</DisplayName>
      <Description>ミッションメッセージ.xlsのテキストID</Description>
      <EditFlags>None</EditFlags>
      <Maximum>9900000</Maximum>
    </Field>
    <Field Def="s16 Priority = 100">
      <DisplayName>優先度</DisplayName>
      <Description>優先度（大きいほうが優先度高い）</Description>
      <EditFlags>None</EditFlags>
      <Minimum>0</Minimum>
      <Maximum>200</Maximum>
    </Field>
    <Field Def="s16 Message Display Limit = -1">
      <DisplayName>メッセージの表示制限</DisplayName>
      <Description>メッセージの表示回数制限（－１が無制限）</Description>
      <EditFlags>None</EditFlags>
      <Minimum>-1</Minimum>
      <Maximum>1000</Maximum>
    </Field>
    <Field Def="u8 Reject message interruptions = 0">
      <DisplayName>メッセージの割り込み拒否</DisplayName>
      <Enum>ON_OFF</Enum>
      <Description>このメッセージ再生中にどんな割り込みも拒否する</Description>
      <EditFlags>None</EditFlags>
      <Maximum>1</Maximum>
    </Field>
    <Field Def="u8 Interrupt messages = 0">
      <DisplayName>メッセージに割り込みをかける</DisplayName>
      <Enum>ON_OFF</Enum>
      <Description>現在再生中のメッセージ以上の優先度をもつメッセージの場合、再生中のメッセージを停止し、再生する。</Description>
      <EditFlags>None</EditFlags>
      <Maximum>1</Maximum>
    </Field>
    <Field Def="fixstr reserve[2]">
      <DisplayName>予約</DisplayName>
      <Description>予約</Description>
      <EditFlags>None</EditFlags>
    </Field>
    <Field Def="f32 Minimum message display interval">
      <DisplayName>メッセージの最低表示間隔</DisplayName>
      <Description>２度続けざまにメッセージリクエストを出したとき、１度目のメッセージが表示されてから、２度目のメッセージが表示されるまでの時間</Description>
      <EditFlags>None</EditFlags>
      <Minimum>0</Minimum>
      <Maximum>1000</Maximum>
    </Field>
    <Field Def="f32 Valid wait time from message issue = -1">
      <DisplayName>メッセージ発行からの有効待ち時間</DisplayName>
      <Description>同時表示数の制限で発行後待ち状態に入った場合、メッセージ表示を断念する時間</Description>
      <EditFlags>None</EditFlags>
      <Minimum>-1</Minimum>
      <Maximum>1000</Maximum>
    </Field>
    <Field Def="f32 Accumulated time until message start">
      <DisplayName>メッセージ開始までの溜め時間</DisplayName>
      <Description>メッセージ再生が決定されてから実際に再生されるまでの時間</Description>
      <EditFlags>None</EditFlags>
      <Minimum>0</Minimum>
      <Maximum>1000</Maximum>
    </Field>
    <Field Def="f32 Message lingering time">
      <DisplayName>メッセージ余韻時間</DisplayName>
      <Description>メッセージ音声終了から実際に終了するまでの余韻時間</Description>
      <EditFlags>None</EditFlags>
      <Minimum>0</Minimum>
      <Maximum>1000</Maximum>
    </Field>
    <Field Def="u32 Monitored message ID0 = 0">
      <DisplayName>監視メッセージID0</DisplayName>
      <Description>監視対象のメッセージID（あるメッセージを再生したら、別のメッセージを再生しないといった用途で使用する）</Description>
      <EditFlags>None</EditFlags>
      <Maximum>9900000</Maximum>
    </Field>
    <Field Def="u32 Monitored message ID1 = 0">
      <DisplayName>監視メッセージID1</DisplayName>
      <Description>監視メッセージＩＤ</Description>
      <EditFlags>None</EditFlags>
      <Maximum>9900000</Maximum>
    </Field>
    <Field Def="u32 Monitored message ID2 = 0">
      <DisplayName>監視メッセージID2</DisplayName>
      <Description>監視メッセージＩＤ</Description>
      <EditFlags>None</EditFlags>
      <Maximum>9900000</Maximum>
    </Field>
    <Field Def="u32 Monitored message ID3 = 0">
      <DisplayName>監視メッセージID3</DisplayName>
      <Description>監視メッセージＩＤ</Description>
      <EditFlags>None</EditFlags>
      <Maximum>9900000</Maximum>
    </Field>
    <Field Def="u32 Monitored message ID4 = 0">
      <DisplayName>監視メッセージID4</DisplayName>
      <Description>監視メッセージＩＤ</Description>
      <EditFlags>None</EditFlags>
      <Maximum>9900000</Maximum>
    </Field>
    <Field Def="u32 Monitored message ID5 = 0">
      <DisplayName>監視メッセージID5</DisplayName>
      <Description>監視メッセージＩＤ</Description>
      <EditFlags>None</EditFlags>
      <Maximum>9900000</Maximum>
    </Field>
    <Field Def="u32 Monitored message ID6 = 0">
      <DisplayName>監視メッセージID6</DisplayName>
      <Description>監視メッセージＩＤ</Description>
      <EditFlags>None</EditFlags>
      <Maximum>9900000</Maximum>
    </Field>
    <Field Def="u32 Monitored message ID7 = 0">
      <DisplayName>監視メッセージID7</DisplayName>
      <Description>監視メッセージＩＤ</Description>
      <EditFlags>None</EditFlags>
      <Maximum>9900000</Maximum>
    </Field>
  </Fields>
</PARAMDEF>