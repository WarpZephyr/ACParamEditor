<?xml version="1.0" encoding="utf-8"?>
<PARAMDEF XmlVersion="3">
  <ParamType>DESTROY_FALL_PARAM_ST</ParamType>
  <DataVersion>1</DataVersion>
  <BigEndian>True</BigEndian>
  <Unicode>False</Unicode>
  <FormatVersion>101</FormatVersion>
  <Fields>
    <Field Def="f32 RotateRevolution">
      <DisplayName>回転角速度[deg/sec]</DisplayName>
      <Minimum>0</Minimum>
      <Maximum>360</Maximum>
      <Increment>0.1</Increment>
    </Field>
    <Field Def="f32 RotateRevolution_RandomRange">
      <DisplayName>回転角速度ランダム幅[deg/sec]</DisplayName>
      <Minimum>0</Minimum>
      <Maximum>360</Maximum>
      <Increment>0.1</Increment>
    </Field>
    <Field Def="f32 FallAcceleration = 9.8">
      <DisplayName>落下加速度[m/sec]</DisplayName>
      <Minimum>0</Minimum>
      <Maximum>100</Maximum>
      <Increment>0.1</Increment>
    </Field>
    <Field Def="f32 FallAcceleration_RandomRange">
      <DisplayName>落下加速度ランダム幅[m/sec]</DisplayName>
      <Minimum>0</Minimum>
      <Maximum>100</Maximum>
      <Increment>0.1</Increment>
    </Field>
    <Field Def="f32 SpreadSpeed">
      <DisplayName>散乱速度[m/sec]</DisplayName>
      <Minimum>0</Minimum>
      <Maximum>100</Maximum>
      <Increment>0.1</Increment>
    </Field>
    <Field Def="f32 SpreadSpeed_RandomRange">
      <DisplayName>散乱速度ランダム幅[m/sec]</DisplayName>
      <Minimum>0</Minimum>
      <Maximum>100</Maximum>
      <Increment>0.1</Increment>
    </Field>
    <Field Def="u8 bCollide = 0">
      <DisplayName>当たり判定</DisplayName>
      <Enum>ON_OFF</Enum>
      <Maximum>1</Maximum>
    </Field>
    <Field Def="s8 DispTimeOnGround = 0">
      <DisplayName>着地後表示時間[sec]</DisplayName>
      <Description>マイナス値にするとずっと表示</Description>
      <Minimum>-1</Minimum>
    </Field>
    <Field Def="u16 RecoilRate = 100">
      <DisplayName>吹っ飛び具合(%)</DisplayName>
    </Field>
    <Field Def="u16 RotationRate = 100">
      <DisplayName>回転具合(%)</DisplayName>
    </Field>
    <Field Def="u8 bSFXDirOutside = 0">
      <DisplayName>ＳＦＸを外側に向かわせるか？</DisplayName>
      <Enum>ON_OFF</Enum>
      <Maximum>1</Maximum>
    </Field>
    <Field Def="u8 bApplyBulletForce = 0">
      <DisplayName>被弾方向に吹っ飛ぶか？</DisplayName>
      <Enum>ON_OFF</Enum>
      <Maximum>1</Maximum>
    </Field>
    <Field Def="u16 TAEScenarioID = 0">
      <DisplayName>TAEのシナリオＩＤ</DisplayName>
      <Description>TimeActEditorのシナリオを呼び出す。</Description>
      <Maximum>9999</Maximum>
    </Field>
    <Field Def="dummy8 reserved[14]">
      <DisplayName>予約</DisplayName>
    </Field>
  </Fields>
